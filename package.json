{
  "name": "artboard",
  "version": "2.0.0",
  "description": "artboard",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "nx exec -- vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "nx exec -- vitest --run",
    "test:watch": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "the-new-css-reset": "^1.8.4",
    "three": "^0.153.0"
  },
  "devDependencies": {
    "@gb-nx/svelte": "7.0.1",
    "@gb-nx/workspace": "4.0.1",
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@types/geojson": "^7946.0.14",
    "@types/three": "^0.152.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-gb": "^2.0.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "happy-dom": "^10.10.4",
    "nx": "17.2.8",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit",
          "{projectRoot}/build"
        ]
      },
      "test": {
        "outputs": [
          "{projectRoot}/coverage"
        ]
      }
    }
  },
  "repository": "github:GaryB432/artboard",
  "bugs": "https://github.com/GaryB432/artboard/issues"
}